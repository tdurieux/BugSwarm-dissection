f(g())
f(h(b))
f(h(c))

b = None # type: B
c = None # type: C

def f(a: 'A[B]') -> None: pass

def g() -> 'A[T]': pass
def h(a: T) -> 'A[T]': pass

class A(Generic[T]): pass
class B: pass
class C(B): pass
[out]
